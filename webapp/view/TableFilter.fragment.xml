<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleTableFilterConfirm">
		<sortItems>
			<ViewSettingsItem text="{i18n>filterReport}" key="jobname" selected="true" />
			<ViewSettingsItem text="{i18n>filterCreatedAt}" key="createdAt" />
			<ViewSettingsItem text="{i18n>filterJobDate}" key="jobDate" />
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>